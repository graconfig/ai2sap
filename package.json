{
  "name": "ai2sap",
  "version": "1.0.0",
  "description": "Generated by cds-services-archetype",
  "license": "ISC",
  "repository": "https://github.com/graconfig/ai2sap.git",
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-ai-sdk/ai-api": "^1.10.0",
    "@sap-ai-sdk/foundation-models": "^1.10.0",
    "@sap-cloud-sdk/connectivity": "^3",
    "@sap-cloud-sdk/http-client": "3.25.0",
    "@sap-cloud-sdk/resilience": "3.25.0",
    "@sap/cds": "^8.9.2",
    "@sap/xssec": "^4",
    "cap-llm-plugin": "^1.4.4",
    "express": "^4",
    "marked": "^15.0.4",
    "passport": "^0.7.0",
    "patch-package": "^8.0.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.14.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/cds-types": "^0.9.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8.9.2",
    "@types/node": "^22.0.0",
    "cds-launchpad-plugin": "^2.2.0",
    "typescript": "^5",
    "typescript-json-schema": "^0.65.1"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "pnpm cds-build && pnpm compile && pnpm cleanup",
    "cds-build": "cds build --production",
    "compile": "tsc",
    "cleanup": "rm -f ./dist/srv/srv/**/*.ts ./dist/srv/package-lock.json",
    "postbuild": "pushd ./dist/srv && npm i --package-lock-only && popd",
    "postinstall": "patch-package",
    "start": "npx cds-serve",
    "watch": "cds-tsx watch",
    "watch:hybrid": "cds-tsx watch --profile hybrid",
    "lint": "eslint . && prettier . --config ../.prettierrc --ignore-path ../.prettierignore -c",
    "lint:fix": "eslint . --fix && prettier . --config ../.prettierrc --ignore-path ../.prettierignore -w --log-level error",
    "watch-convert": "cds watch --open convert/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "query": {
      "limit": {
        "reliablePaging": true
      }
    },
    "requires": {
      "[production]": {
        "auth": "xsuaa",
        "db": {
          "kind": "hana"
        }
      },
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "db/ai2sap.sqlite"
        }
      }
    },
    "destinations": true,
    "html5-repo": true,
    "workzone": true,
    "sql": {
      "native_hana_associations": false
    }
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "sapux": [
    "app/runtime",
    "app/convert",
    "app/convert"
  ]
}
